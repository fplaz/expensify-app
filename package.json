    "css-loader": "^6.7.1",
    "got": "^12.3.1",
    "live-server": "^1.1.0",
    "moment": "^2.29.4",
    "my-ui-lib": "^1.2.2",
    "node-sass": "^7.0.1",
    "normalize.css": "^8.0.1",
